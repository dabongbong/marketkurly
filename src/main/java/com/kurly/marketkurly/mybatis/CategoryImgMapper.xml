<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="CategoryImg">

	<insert id="insert" parameterType="CategoryImg">
		insert into category_img(category_id, img) values( #{category.category_id}, #{img})
	</insert> 
</mapper>